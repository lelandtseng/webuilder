            <script>
                $(function(){
                    $('#slide').slider({
                        value: 80,
                        min: 80,
                        max: 350,
                        step: 10,
                        slide: function(event, ui){
                            $('#header').css('height', ui.value + 'px');
                        }
                    });
                    $('#btl-show-image').click(function(){
                        $('#header').css('background', 'url(http://www.google.com.hk/intl/zh-CN/images/logo_cn.png)');
                    });
                    
                    $('#btl-hide-image').click(function(){
                        $('#header').css('background', 'none');
                    });
                })
            </script>
